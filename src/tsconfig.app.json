{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "module": "es2015",
    "sourceMap": true,

    "baseUrl": "",
    "types": []
  },
  "paths": {
    "@app/*": ["app/*"],
    "@env/*": [
      "environments/*"
    ],
    "@testing/*": [
      "testing/*"
    ]
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts",
    "dist",
    "app-builds",
    "node_modules"
  ]
}
